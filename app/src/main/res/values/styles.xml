<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Text_White" >
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="Text_black" >
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="text_black_12_bold" parent="Text_black">
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Text_White_16" parent="Text_White">
        <item name="android:textSize">16sp</item>
        <item name="fontFamily">sans-serif-medium</item>
    </style>
    <style name="Text_White_13" parent="Text_White_16">
        <item name="android:textSize">13sp</item>
    </style>
    <style name="Text_White_14" parent="Text_White_16">
        <item name="android:textSize">14sp</item>
    </style>
    <style name="Text_White_15" parent="Text_White_16">
        <item name="android:textSize">15sp</item>
    </style>
    <style name="Text_White_32" parent="Text_White">
        <item name="android:textSize">32sp</item>
        <item name="font">@font/water_splash</item>
    </style>
    <style name="circleImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="spinnerItemStyle" parent="android:Widget.TextView.SpinnerItem">
        <item name="android:textSize">14sp</item>
        <item name="fontFamily">sans-serif-medium</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="spinnerDropDownItemStyle" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
        <item name="android:textSize">14sp</item>
        <item name="fontFamily">sans-serif-medium</item>
        <item name="android:textColor">@color/white</item>
    </style>
</resources>
